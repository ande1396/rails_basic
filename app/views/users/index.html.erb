<% provide :title, "Users" %>

<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>


<table>

<tr>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Email Address</th>
</tr>

<tr>
	<td>
		<% @users.each do |user| %>
		<%= user.first_name %>
		<%= user.last_name %>
		<%= user.email_address %>
	</td>
</tr>

<% end %> 
</table>